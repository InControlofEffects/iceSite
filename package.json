{
  "name": "ice-site",
  "version": "1.0.0",
  "description": "static site for web app",
  "main": "index.js",
  "dependencies": {
    "async_hooks": "^1.0.0",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-ga": "^2.7.0",
    "react-markdown": "^4.2.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development parcel src/index.html",
    "build": "NODE_ENV=production parcel build src/index.html --public-url ./ --out-dir public",
    "clean": "rm -rf dist .cache public"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidruvolo51/ice-site.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidruvolo51/ice-site/issues"
  },
  "homepage": "https://github.com/davidruvolo51/ice-site#readme"
}
